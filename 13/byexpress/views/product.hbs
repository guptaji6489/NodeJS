<h1>{{title}}</h1>

<table class="table">
    <tr>
        <th>id</th>
        <th>roll no</th>
        <th>name</th>
        <th>city</th>
        <th>standard</th>
    </tr>
    {{#each product}}
    <tr>
        <td>{{this.id}}</td>
        <td>{{this.rollno}}</td>
        <td>{{this.name}}</td>
        <td>{{this.city}}</td>
        <td>{{this.standard}}</td>
    </tr>
    {{/each}}
</table>