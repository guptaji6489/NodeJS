<div class="container">
  <h1>CRUD Operations by Guptaji</h1>
  <h2>{{title}}</h2>
  <a href="/product/create-form" class="btn btn-success">Create Product</a>
  <br /><br />
<table class="table">
    <tr>
        <th>id</th>
        <th>roll no</th>
        <th>name</th>
        <th>city</th>
        <th>standard</th>
    </tr>
    {{#each product}}
    <tr>
        <td>{{this.id}}</td>
        <td>{{this.rollno}}</td>
        <td>{{this.name}}</td>
        <td>{{this.city}}</td>
        <td>{{this.standard}}</td>
        <td>
            <a href="/product/edit-form/{{this.id}}" class="btn btn-info btn-sm">Edit</a>
            <a href="/product/delete/{{this.id}}" class="btn btn-danger btn-sm">Delete</a>
        </td>
    </tr>
    {{/each}}
</table>


</div>